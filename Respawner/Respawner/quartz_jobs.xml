<?xml version="1.0" encoding="UTF-8"?>

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
    <processing-directives>
        <overwrite-existing-data>true</overwrite-existing-data>
    </processing-directives>

    <schedule>
        <job>
            <name>RespawnJob</name>
            <group>RespawnJobGroup</group>
            <description>Every 10 minutes output</description>
            <job-type>Respawner.Jobs.RespawnJob, Respawner</job-type>
            <durable>true</durable>
            <recover>false</recover>
        </job>

        <trigger>
            <cron>
                <name>RespawnJobTrigger</name>
                <group>RespawnJobTriggergroup</group>
                <description>Every 10 minutes output</description>
                <job-name>RespawnJob</job-name>
                <job-group>RespawnJobGroup</job-group>
                <misfire-instruction>SmartPolicy</misfire-instruction>
                <cron-expression>0 */1 * * * ?</cron-expression>
            </cron>
        </trigger>
    </schedule>
</job-scheduling-data>